	  "@id": "http://ontology.cybershare.utep.edu/livinglab/scllv",
	  "@type": "iot:Model",
	  "label" : "Smart Cities Living Lab Vocabulary",
	  "iot:attribute": [
		{
		  "@type": "iot:Attribute",
		  "@id": "#temperature",
		  "iot:purpose": "iot-purpose:temperature",
		  "iot:unit": "iot-unit:temperature.si.celsius",
		  "iot:type": "iot:type.number",
		  "iot:read": true,
		  "iot:write": false,
		  "iot:sensor": true,
		  "iot:actuator": false,
		  "label" : "temperature",
		  "comment" : "numeric temperature value measured in celsius by a temperature sensor"
		},
		{
		  "@type": "iot:Attribute",
		  "@id": "#pressure",
		  "iot:purpose": "iot-purpose:pressure",
		  "iot:unit": "iot-unit:pressure.si.pascal.3",
		  "iot:type": "iot:type.number",
		  "iot:read": true,
		  "iot:write": false,
		  "iot:sensor": true,
		  "iot:actuator": false,
		  "label" : "pressure",
		  "comment" : "numeric pressure value measured un Kilo Pascal by a barometer sensor"
		},
		{
		  "@type": "iot:Attribute",
		  "@id": "#noise",
		  "iot:purpose": "iot-purpose:sound",
		  "iot:unit": "iot-unit:math.fraction.percent",
		  "iot:type": "iot:type.number",
		  "iot:read": true,
		  "iot:write": false,
		  "iot:sensor": true,
		  "iot:actuator": false,
		  "label" : "noise",
		  "comment" : "numeric noise value measured in decibels by a microphone sensor"
		},
		{
		  "@type": "iot:Attribute",
		  "@id": "#light",
		  "iot:purpose": "iot-purpose:light",
		  "iot:unit": "iot-unit:math.fraction.percent",
		  "iot:type": "iot:type.number",
		  "iot:read": true,
		  "iot:write": false,
		  "iot:sensor": true,
		  "iot:actuator": false,
		  "label" : "light",
		  "comment" : "numeric light value measured in lumens by a light sensor"
		}
	  ],
	  "iot:meta" : [
		{ "@type": "owl:Class",
		  "@id": "#board",
		  "label" : "board",
		  "comment" : "development prototyping board based on microcontroller architectures"
		},
		{ "@type": "owl:DatatypeProperty",
		  "@id": "#serialNumber",
		  "label" : "serial number",
		  "comment" : " a unique identifier assigned incrementally or sequentially to an item"
		},
		{ "@type": "owl:DatatypeProperty",
		  "@id": "#macAddress",
		  "label" : "media access control address",
		  "comment" : "a unique identifier assigned tonetwork interfaces for communications on the physical network segment"
		},
		{ "@type": "owl:DatatypeProperty",
		  "@id": "#protocol",
		  "label" : "protocol",
		  "comment" : "is a system of rules that allow two or more entities of a communications system to transmit information via any kind of variation of a physical quantity"
		},
	    { "@type": "owl:DatatypeProperty",
		  "@id": "#partName",
		  "label" : "part name",
		  "comment" : "a word that names a part of a larger whole"
		},
	    { "@type": "owl:DatatypeProperty",
		  "@id": "#deviceType",
		  "label" : "type",
		  "comment" : "Signal type used by a device (analog or digital)"
		}
